    "# Tokenizing"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "#Function for tokenizing\n",
    "def tokenize_adv(raw_data):\n",
    "    \n",
    "    low_data = raw_data.lower()#converted to lower case\n",
    "    sentences= sent_tokenize(low_data)\n",
    "    pattern = r\"[a-zA-Z]+(?:[-'][a-zA-Z]+)?\" #regex pattern \n",
    "    tokenizer = RegexpTokenizer(pattern)\n",
    "    token_lists = [tokenizer.tokenize(sen) for sen in sentences]\n",
    "    tokenized_data = list(chain.from_iterable(token_lists))\n",
    "    return tokenized_data    "
   ]
  },
